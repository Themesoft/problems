<div class="slider" movement="3">
  <h3>The Slider</h3>
  <div >
    <%= @project.slider_objects.inspect %>
    <% @project.slider_objects.each_with_index do |pa, i| %>
        <li class="resource text-center">
        <%= link_to pa.resource_url, target: "_blank" do %>
          <%= "Resource #{i+1}" %>
        <% end %>
        </li>
    <% end %>
  </div>
</div>
